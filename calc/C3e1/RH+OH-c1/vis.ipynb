    "from pathlib import Path\n",
    "\n",
    "import automol\n",
    "\n",
    "geo_file = Path(\"opt_trj.xyz\")\n",
    "geos, comments = zip(\n",
    "    *automol.geom.from_xyz_trajectory_string(geo_file.read_text()),\n",
    "    strict=True,\n",
    ")\n",
    "automol.geom.display_trajectory(geos)"
   ]
